{
    "name": "@cartesi/rollups",
    "version": "2.1.1-alpha.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/cartesi/rollups-contracts"
    },
    "scripts": {
        "build:contracts": "forge build src",
        "build:devnet": "node script/build-devnet.mjs",
        "build:devnet-no-cannon": "forge script script/Deploy.s.sol:Deploy --rpc-url http://127.0.0.1:8545 --broadcast --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
        "ci:publish": "changeset publish",
        "ci:version": "changeset version && node script/update-cannonfile-version.mjs",
        "clean": "pnpm clean:forge && pnpm clean:cannon",
        "clean:cannon": "rm -rf deployments state.json",
        "clean:forge": "forge clean",
        "fmt:check": "forge fmt --check",
        "fmt:write": "forge fmt",
        "prepack": "pnpm clean && pnpm build:contracts"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.7",
        "@usecannon/cli": "^2.25.1"
    },
    "files": [
        "out"
    ],
    "license": "Apache-2.0",
    "packageManager": "pnpm@10.11.0"
}
